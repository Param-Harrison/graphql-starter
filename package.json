  },
  "dependencies": {
    "babel-cli": "6.24.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-env": "1.2.2",
    "body-parser": "1.17.1",
    "compression": "1.6.2",
    "cors": "2.8.1",
    "express": "4.15.2",
    "graphql": "0.9.1",
    "graphql-server-express": "0.6.0",
    "graphql-tools": "0.10.1",
    "morgan": "1.8.1",
    "rimraf": "2.6.1"
  }
